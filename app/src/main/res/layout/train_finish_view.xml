<?xml version="1.0" encoding="utf-8"?>
<com.uyu.device.devicetraining.presentation.view.adapter.selfhelp.CheckLinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_ee"
    android:gravity="center"
    android:orientation="vertical">
    <TextView
        android:id="@+id/finish_tips_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:padding="5dp"
        android:textColor="@color/theme_font_color"
        android:textSize="20sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/next_train_item_btn"
        android:layout_width="220dp"
        android:layout_height="40dp"
        android:layout_gravity="center"
        android:layout_marginTop="10dp"
        android:background="@drawable/dotted_line_bg"
        android:text="@string/next_train"
        android:textColor="@color/theme_font_color" />

    <ImageView
        android:layout_width="150dp"
        android:layout_height="200dp"
        android:src="@drawable/train_end_bottom"
        android:layout_margin="30dp"
        />

</com.uyu.device.devicetraining.presentation.view.adapter.selfhelp.CheckLinearLayout>