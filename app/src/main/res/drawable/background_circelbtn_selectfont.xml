<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item >
        <shape
            android:shape="oval"
            android:useLevel="false">
            <solid android:color="#5BBFCE" />
            <size
                android:width="40dp"
                android:height="40dp" />
        </shape>
    </item>

    <item android:top="3dp" android:bottom="3dp" android:left="3dp" android:right="3dp">
        <shape
            android:shape="oval"
            android:useLevel="false">
            <solid android:color="#039DB3" />
            <size
                android:width="40dp"
                android:height="40dp" />
        </shape>
    </item>
</layer-list>